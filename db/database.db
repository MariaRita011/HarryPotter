CREATE TABLE bruxos (id SERIAL PRIMARY KEY, nome VARCHAR(100) NOT NULL, idade INT NOT NULL, casa_hogwarts VARCHAR(15) NOT NULL, habilidade_especial VARCHAR(100) NOT NULL, tipo_sangue VARCHAR(10) NOT NULL, patrono VARCHAR(100));

CREATE TABLE varinhas (id SERIAL PRIMARY KEY, material VARCHAR(100) NOT NULL, comprimento INT NOT NULL, nucleo VARCHAR(100) NOT NULL, data_fabricacao DATE NOT NULL);